        },
        "zh_plain_search": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
                "smartcn_stop",
                "icu_normalizer"
            ],
            "char_filter": [
                "word_break_helper"
            ]
        },
        "sv_plain": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
                "icu_normalizer",
                "preserve_original_recorder",
                "sv_icu_folding",
                "preserve_original",
                "remove_empty"
            ],
            "char_filter": [
                "word_break_helper"
            ]
        },
        "sv_plain_search": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
                "icu_normalizer"
