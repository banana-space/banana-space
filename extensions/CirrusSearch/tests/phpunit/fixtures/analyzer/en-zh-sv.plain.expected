                "icu_normalizer",
                "preserve_original_recorder",
                "icu_folding",
                "preserve_original",
                "remove_empty"
            ],
            "char_filter": [
                "word_break_helper"
            ]
        },
        "en_plain_search": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
                "icu_normalizer"
            ],
            "char_filter": [
                "word_break_helper"
            ]
        },
        "zh_plain": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
                "smartcn_stop",
                "icu_normalizer"
            ],
            "char_filter": [
                "word_break_helper"
