            "char_filter": [
                "word_break_helper"
            ]
        }
    },
    "filter": {
        "icu_normalizer": {
            "type": "icu_normalizer",
            "name": "nfkc_cf"
        },
        "icu_folding": {
            "type": "icu_folding"
        },
        "remove_empty": {
            "type": "length",
            "min": 1
        },
        "smartcn_stop": {
            "type": "stop",
            "stopwords": [
                ","
            ]
        },
        "sv_icu_folding": {
            "type": "icu_folding",
            "unicodeSetFilter": "[^\u00e5\u00e4\u00f6\u00c5\u00c4\u00d6]"
        }
    },
    "char_filter": {
        "word_break_helper": {
            "type": "mapping",
            "mappings": [
                "_=>\\u0020",
                ".=>\\u0020",
                "(=>\\u0020",
                ")=>\\u0020"
            ]
        }
    },
    "tokenizer": []
}