{
	"name": "TeXParser",
	"author": "alphaGem",
	"url": "",
	"description": "Miau!",
	"version": "0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "parser",
	"manifest_version": 1,
	"namespaces": [
		{
			"id": 100,
			"constant": "NS_NOTES",
			"name": "讲义",
			"content": true
		},
		{
			"id": 101,
			"constant": "NS_NOTES_TALK",
			"name": "讲义讨论",
			"defaultcontentmodel": "flow-board"
		}
	],
	"AutoloadClasses": {
		"SubpageHandler": "includes/SubpageHandler.php",
		"TeXParserHooks": "includes/TeXParserHooks.php",
		"EditorHooks": "includes/EditorHooks.php"
	},
	"ExtensionFunctions": [
		"TeXParserHooks::initExtension"
	],
	"ExtensionMessagesFiles": {
		"ExampleExtensionMagic": "includes/TeXParser.i18n.php"
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "TeXParserHooks::onLoadExtensionSchemaUpdates",
		"ParserBeforeInternalParse": "TeXParserHooks::onParserBeforeInternalParse",
		"PageSaveComplete": "TeXParserHooks::onPageSaveComplete",
		"OutputPageParserOutput": "TeXParserHooks::onOutputPageParserOutput",
		"BeforePageDisplay": "TeXParserHooks::onBeforePageDisplay",
		"ParserFirstCallInit": "TeXParserHooks::onParserFirstCallInit",
		"EditPage::showEditForm:initial": "EditorHooks::onEditPageShowEditFormInitial",
		"EditPage::showReadOnlyForm:initial": "EditorHooks::onEditPageShowReadOnlyFormInitial"
	},
	"ResourceModules": {
		"ext.TeXParser": {
			"packageFiles": [ "extensions/TeXParser/resources/ext.KaTeXcss.js" ],
			"styles": [ "extensions/TeXParser/resources/ext.TeXParser.less" ]
		}
	}
}
