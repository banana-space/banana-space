{
	"name": "TeXParser",
	"author": "alphaGem",
	"url": "",
	"description": "Miau!",
	"version": "0.1",
	"license-name": "GPL-2.0-or-later",
	"type": "parser",
	"manifest_version": 1,
	"AutoloadClasses": {
		"SubpageHandler": "includes/SubpageHandler.php",
		"TeXParserHooks": "includes/TeXParserHooks.php"
	},
	"ExtensionFunctions": [
		"TeXParserHooks::initExtension"
	],
	"Hooks": {
		"LoadExtensionSchemaUpdates": "TeXParserHooks::onLoadExtensionSchemaUpdates",
		"ParserBeforeInternalParse": "TeXParserHooks::onParserBeforeInternalParse",
		"PageSaveComplete": "TeXParserHooks::onPageSaveComplete",
		"BeforePageDisplay": "TeXParserHooks::onBeforePageDisplay"
	},
	"ResourceModules": {
		"ext.TeXParser": {
			"packageFiles":"extensions/TeXParser/resources/ext.KaTeXcss.js",
			"styles":["extensions/TeXParser/resources/ext.TeXParser.less"]
		}
	}
}
